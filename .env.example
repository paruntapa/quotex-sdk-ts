# Quotex SDK Configuration
# Copy this file to .env.local and fill in your credentials

# Your Quotex account credentials
QUOTEX_EMAIL=your@email.com
QUOTEX_PASSWORD=yourpassword

# Language (en, pt, es)
QUOTEX_LANG=en

# Account mode (PRACTICE for demo, REAL for live trading)
# ALWAYS start with PRACTICE to test!
QUOTEX_MODE=PRACTICE

# Enable debug mode (true/false)
DEBUG=true
