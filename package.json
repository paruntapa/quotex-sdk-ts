{
  "name": "quotex-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Quotex trading platform - built with Bun",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun --format esm --minify",
    "dev": "bun --watch src/index.ts",
    "example:basic": "bun run examples/basic-usage.ts",
    "example:trading": "bun run examples/trading-bot.ts",
    "example:analysis": "bun run examples/technical-analysis.ts",
    "example:monitor": "bun run examples/market-monitoring.ts",
    "create-session": "bun run create-manual-session.ts"
  },
  "keywords": [
    "quotex",
    "trading",
    "sdk",
    "typescript",
    "bun",
    "binary-options",
    "technical-indicators",
    "websocket",
    "api-client"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "playwright": "^1.56.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
